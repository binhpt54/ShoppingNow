<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true" android:state_focused="true"
        android:drawable="@drawable/rd_focused"></item>
    <item android:state_checked="true" android:state_enabled="false" 
        android:drawable="@drawable/rd_disable"></item>
    <item android:state_checked="true" android:state_pressed="true"
        android:drawable="@drawable/rd_pressed"></item>
    <item android:state_checked="true"
        android:drawable="@drawable/rd_normal"></item>
    
    <item android:state_checked="false" android:state_focused="true"
        android:drawable="@drawable/rd_unselected_focused"></item>
    <item android:state_checked="false" android:state_enabled="false"
        android:drawable="@drawable/rd_unselected_disable"></item>
    <item android:state_checked="false" android:state_focused="true" android:state_enabled="false"
        android:drawable="@drawable/rd_unselected_disable_focused"></item>
    <item android:state_checked="false"
        android:drawable="@drawable/rd_uncheck_normal"></item>
    <item android:state_checked="false" android:state_pressed="true"
        android:drawable="@drawable/rd_unselected_pressed"></item>
    
    

</selector>